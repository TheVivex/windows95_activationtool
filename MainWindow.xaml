<Window x:Class="win95__keygenerator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:win95__keygenerator"
        mc:Ignorable="d"
        Title="Windows 95 Activation Tool" Height="298" Width="333" ResizeMode="NoResize">
    <Grid>
        <TextBox x:Name="key_tb" HorizontalAlignment="Left" Margin="10,91,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="284" FontSize="22"/>
        <Button x:Name="gen_bt" Content="Generate" HorizontalAlignment="Left" Margin="17,141,0,0" VerticalAlignment="Top" Click="gen_bt_Click" Width="99" FontSize="18"/>
        <ComboBox x:Name="ver_cb" HorizontalAlignment="Left" Margin="17,34,0,0" VerticalAlignment="Top" Width="83" SelectionChanged="ver_cb_SelectionChanged"/>
        <Label Content="Windows 95 version:" HorizontalAlignment="Left" Margin="10,11,0,0" VerticalAlignment="Top"/>
        <Label x:Name="data" Content="Bartosz Brzostek 2023" HorizontalAlignment="Left" Margin="186,235,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
